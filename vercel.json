{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
    ],
    "env": {
        "OPENAI_API_KEY": "@openai_api_key",
        "OPENAI_ORG": "@openai_org",
        "ELEVENLABS_KEY": "@elevenlabs_key"
    }
}